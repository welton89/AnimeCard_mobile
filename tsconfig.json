{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "app/components/*"
      ],
      "@app/*": [
        "app/*"
      ],
      "@screens/*": [
        "app/screens/*"
      ],
      "@assets/*": [
        "app/assets/*"
      ],
      "@utils/*": [
        "app/utils/*"
      ]
    },
    "strict": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
